#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/ql/scripts/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/node_modules:/ql/scripts/node_modules/.pnpm/semver@5.7.1/node_modules/semver/node_modules:/ql/scripts/node_modules/.pnpm/semver@5.7.1/node_modules:/ql/scripts/node_modules/.pnpm/node_modules:/ql/scripts/node_modules:/ql/node_modules:/node_modules:/ql/scripts/node_modules/.pnpm/make-dir@2.1.0/node_modules/make-dir/node_modules:/ql/scripts/node_modules/.pnpm/make-dir@2.1.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../semver@5.7.1/node_modules/semver/bin/semver" "$@"
else
  exec node  "$basedir/../../../../../semver@5.7.1/node_modules/semver/bin/semver" "$@"
fi
