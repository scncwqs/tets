#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/ql/scripts/node_modules/.pnpm/sshpk@1.16.1/node_modules/sshpk/bin/node_modules:/ql/scripts/node_modules/.pnpm/sshpk@1.16.1/node_modules/sshpk/node_modules:/ql/scripts/node_modules/.pnpm/sshpk@1.16.1/node_modules:/ql/scripts/node_modules/.pnpm/node_modules:/ql/scripts/node_modules:/ql/node_modules:/node_modules:/ql/scripts/node_modules/.pnpm/node_modules/sshpk/bin/node_modules:/ql/scripts/node_modules/.pnpm/node_modules/sshpk/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sshpk/bin/sshpk-conv" "$@"
else
  exec node  "$basedir/../sshpk/bin/sshpk-conv" "$@"
fi
