#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/ql/scripts/node_modules/.pnpm/seek-bzip@1.0.6/node_modules/seek-bzip/bin/node_modules:/ql/scripts/node_modules/.pnpm/seek-bzip@1.0.6/node_modules/seek-bzip/node_modules:/ql/scripts/node_modules/.pnpm/seek-bzip@1.0.6/node_modules:/ql/scripts/node_modules/.pnpm/node_modules:/ql/scripts/node_modules:/ql/node_modules:/node_modules:/ql/scripts/node_modules/.pnpm/node_modules/seek-bzip/bin/node_modules:/ql/scripts/node_modules/.pnpm/node_modules/seek-bzip/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../seek-bzip/bin/seek-bunzip" "$@"
else
  exec node  "$basedir/../seek-bzip/bin/seek-bunzip" "$@"
fi
