#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/ql/scripts/node_modules/.pnpm/registry.nlark.com+ecstatic@3.3.2/node_modules/ecstatic/lib/node_modules:/ql/scripts/node_modules/.pnpm/registry.nlark.com+ecstatic@3.3.2/node_modules/ecstatic/node_modules:/ql/scripts/node_modules/.pnpm/registry.nlark.com+ecstatic@3.3.2/node_modules:/ql/scripts/node_modules/.pnpm/node_modules:/ql/scripts/node_modules:/ql/node_modules:/node_modules:/ql/scripts/node_modules/.pnpm/node_modules/ecstatic/lib/node_modules:/ql/scripts/node_modules/.pnpm/node_modules/ecstatic/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ecstatic/lib/ecstatic.js" "$@"
else
  exec node  "$basedir/../ecstatic/lib/ecstatic.js" "$@"
fi
