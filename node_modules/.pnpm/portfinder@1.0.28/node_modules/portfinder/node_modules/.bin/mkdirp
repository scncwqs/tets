#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/ql/scripts/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/bin/node_modules:/ql/scripts/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/node_modules:/ql/scripts/node_modules/.pnpm/mkdirp@0.5.5/node_modules:/ql/scripts/node_modules/.pnpm/node_modules:/ql/scripts/node_modules:/ql/node_modules:/node_modules:/ql/scripts/node_modules/.pnpm/portfinder@1.0.28/node_modules/portfinder/node_modules:/ql/scripts/node_modules/.pnpm/portfinder@1.0.28/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mkdirp@0.5.5/node_modules/mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../../../../../mkdirp@0.5.5/node_modules/mkdirp/bin/cmd.js" "$@"
fi
